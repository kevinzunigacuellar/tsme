{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"rootDir": "src",
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"noEmit": false,
		"useUnknownInCatchVariables": true,
		"moduleResolution": "nodenext",
		"declaration": true
	}
}
